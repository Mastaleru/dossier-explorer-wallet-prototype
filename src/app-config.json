      "icon": "fa-bars",
      "type": "route",
      "component": "psk-page-loader",
      "exact": true,
      "indexed": true,
      "historyType": "browser"
    },
    "pages": [
      {
        "name": "Dashboard",
        "path": "/dashboard",
        "icon": "fa-home",
        "pageSrc": "index.html"
      },
      {
        "name": "E-wallet",
        "path": "/e-wallet",
        "pageSrc": "Wallet/wallet.html"
      },
      {
        "name": "My Apps",
        "path": "/my-apps",
        "icon": "fa-plug",
        "children": "@event:getSSApps"
      },
      {
        "name": "Nested Menu",
        "path": "/nested-menu",
        "icon": "fa-plug",
        "children": [
          {
            "name": "Item1"
          },
          {
            "name": "item2"
          }
        ]
      },
      {
        "name": "404",
        "indexed": false
      }
    ]
  }
}
