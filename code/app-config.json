        "path": "/my-apps",
        "icon": "fa-plug",
        "children": "@event:getSSApps"
      },
      {
        "name": "Nested Menu",
        "path": "/nested-menu",
        "icon": "fa-plug",
        "children": [{
            "name": "Item1"
          },
          {
            "name": "item2"
          }
        ]
      },
      {
        "name": "404",
        "path":"404",
        "indexed": false
      }
    ]
