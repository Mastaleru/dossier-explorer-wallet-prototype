{
  "basePagesUrl": "/pages/",
  "profile": {
    "username": "Dossier Explorer",
    "avatar": "/assets/images/dossier-explorer.png"
  },
  "modals": {
    "signOut": "Wallet/modals/sign-out.html",
    "createDossier": "Wallet/modals/create-dossier.html",
    "importDossier": "Wallet/modals/import-dossier.html",
    "receiveDossier": "Wallet/modals/receive-dossier.html",
    "deleteDossier": "Wallet/modals/delete-dossier.html",
