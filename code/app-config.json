{
  "theme": "dossier-explorer",
  "appVersion": "0.1",
  "basePagesUrl": "/pages/",
  "profile": {
    "username": "Dossier Explorer",
    "avatar": "/assets/images/dossier-explorer.png"
  },
  "modals": {
    "signOut": "Wallet/modals/sign-out.html",
    "createDossier": "Wallet/modals/create-dossier.html",
    "importDossier": "Wallet/modals/import-dossier.html",
    "receiveDossier": "Wallet/modals/receive-dossier.html",
    "deleteDossier": "Wallet/modals/delete-dossier.html",
    "shareDossier": "Wallet/modals/share-dossier.html",
    "renameDossier": "Wallet/modals/rename-dossier.html",
    "viewAsset": "Wallet/modals/view-asset.html",
    "moveDossier": "Wallet/modals/move-dossier.html",
    "runApp": "Wallet/modals/run-app.html"
  },
  "menu": {
    "defaultMenuConfig": {
      "icon": "fa-bars",
      "type": "route",
      "component": "psk-page-loader",
      "exact": true,
      "indexed": true,
      "historyType": "browser"
    },
    "pages": [{
        "name": "Explorer",
        "path": "/explorer",
        "pageSrc": "Wallet/wallet.html"
      },
      {
        "name": "My Apps",
        "path": "/my-apps",
        "icon": "fa-plug",
        "children": "@event:getSSApps",
        "propName":"appName",
        "component": "psk-ssapp"
      }
    ]
  }
}
